{
  "dependencies": {
    "@abandonware/bluetooth-hci-socket": "~0.5.3-10",
    "axios": "~1.6.1",
    "buffer-xor": "~2.0.2",
    "dbus-next": "~0.10.2",
    "fs": "0.0.1-security",
    "jspack": "~0.0.4",
