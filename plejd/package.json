  "scripts": {
    "lint": "npm run lint:prettier & npm run lint:scripts",
    "lint:fix": "npm run lint:prettier:fix & npm run lint:scripts:fix",
    "lint:prettier": "prettier --check --config ../.prettierrc.js \"**/*.{js*,md}\"",
